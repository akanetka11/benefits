    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/SF-UI-Display-Bold.ttf",
            "./src/assets/fonts/SF-UI-Display-Medium.ttf",
            "./src/assets/fonts/SF-UI-Display-Regular.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
